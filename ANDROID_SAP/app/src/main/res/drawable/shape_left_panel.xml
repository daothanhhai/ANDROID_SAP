<?xml version="1.0" encoding="utf-8"?>
<layer-list tools:ignore="NamespaceTypo"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/shadow_left"/>
    <item>
        <shape android:shape="rectangle">

            <corners android:topRightRadius="40dp" android:bottomRightRadius="40dp"/>
            <gradient android:angle="225"
                android:startColor="#0091EA" android:centerColor="#AA00FF"
                android:endColor="#F50057"/>

        </shape>

    </item>
</layer-list>



